        "audio_end": 92640
    },
    {
        "id": "f000035",
        "sentence": "吃驚致しまして、",
        "sentence_with_furigana": "吃驚[びっくり]致しまして、",
        "audio_begin": 92640,
        "audio_end": 94400
    },
    {
