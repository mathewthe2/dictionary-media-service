        "audio_begin": 84751,
        "audio_end": 85760
    },
    {
        "id": "f000030",
        "sentence": "曾祖母さんは、",
        "sentence_with_furigana": "曾祖母[おおばあ]さんは、",
        "audio_begin": 85760,
        "audio_end": 86840
    },
