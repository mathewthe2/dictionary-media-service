[
    {
        "id": "f000001",
        "sentence": "池田蕉園　作",
        "sentence_with_furigana": "池田蕉園　作",
        "audio_begin": 2328,
        "audio_end": 6515
    },
    {
        "id": "f000002",
        "sentence": "「ああしんど」",
        "sentence_with_furigana": "「ああしんど」",
        "audio_begin": 6515,
        "audio_end": 10745
    },
    {
        "id": "f000003",
        "sentence": "よっぽど古いお話なんで御座いますよ。",
        "sentence_with_furigana": "よっぽど古いお話なんで 御座[ござ]いますよ。",
        "audio_begin": 10745,
        "audio_end": 14760
    },
    {
        "id": "f000004",
        "sentence": "私の祖父の子供の時分に居りました、",
        "sentence_with_furigana": "私の 祖父[じじい]の子供の時分に居りました、",
        "audio_begin": 14760,
        "audio_end": 17800
    },
    {
        "id": "f000005",
        "sentence": "「三」という猫なんで御座います。",
        "sentence_with_furigana": "「 三[さん]」という猫なんで 御座[ござ]います。",
        "audio_begin": 17800,
        "audio_end": 20600
    },
    {
        "id": "f000006",
        "sentence": "三毛だったんで御座いますって。",
        "sentence_with_furigana": "三毛[みけ]だったんで 御座[ござ]いますって。",
        "audio_begin": 20600,
        "audio_end": 23520
    },
    {
        "id": "f000007",
        "sentence": "何でも、",
        "sentence_with_furigana": "何でも、",
        "audio_begin": 23520,
        "audio_end": 24760
    },
    {
        "id": "f000008",
        "sentence": "あの、",
        "sentence_with_furigana": "あの、",
        "audio_begin": 24760,
        "audio_end": 25400
    },
    {
        "id": "f000009",
        "sentence": "その祖父の話に、",
        "sentence_with_furigana": "その 祖父[じじい]の話に、",
        "audio_begin": 25400,
        "audio_end": 27400
    },
    {
        "id": "f000010",
        "sentence": "おばあさんがお嫁に来る時に——",
        "sentence_with_furigana": "おばあさんがお嫁に来る時に——",
        "audio_begin": 27400,
        "audio_end": 30360
    },
    {
        "id": "f000011",
        "sentence": "祖父のお母さんなんで御座いましょうねえ——",
        "sentence_with_furigana": "祖父[じじい]のお母さんなんで 御座[ござ]いましょうねえ——",
        "audio_begin": 30360,
        "audio_end": 33960
    },
    {
        "id": "f000012",
        "sentence": "泉州堺から連れて来た猫なんで御座いますって。",
        "sentence_with_furigana": "泉州堺[せんしゅうさかい]から連れて来た猫なんで御座いますって。",
        "audio_begin": 33960,
        "audio_end": 38785
    },
    {
        "id": "f000013",
        "sentence": "随分永く——",
        "sentence_with_furigana": "随分[ずいぶん]永く——",
        "audio_begin": 38785,
        "audio_end": 41275
    },
    {
        "id": "f000014",
        "sentence": "家に十八年も居たんで御座いますよ。",
        "sentence_with_furigana": "家に十八年も居たんで 御座[ござ]いますよ。",
        "audio_begin": 41275,
        "audio_end": 45800
    },
    {
        "id": "f000015",
        "sentence": "大きくなっておりましたそうです。",
        "sentence_with_furigana": "大きくなっておりましたそうです。",
        "audio_begin": 45800,
        "audio_end": 48760
    },
    {
        "id": "f000016",
        "sentence": "もう、",
        "sentence_with_furigana": "もう、",
        "audio_begin": 48760,
        "audio_end": 49280
    },
    {
        "id": "f000017",
        "sentence": "耳なんか、",
        "sentence_with_furigana": "耳なんか、",
        "audio_begin": 49280,
        "audio_end": 50360
    },
    {
        "id": "f000018",
        "sentence": "厚ぼったく、",
        "sentence_with_furigana": "厚ぼったく、",
        "audio_begin": 50360,
        "audio_end": 51280
    },
    {
        "id": "f000019",
        "sentence": "五分ぐらいになっていたそうで御座いますよ。",
        "sentence_with_furigana": "五 分[ぶ]ぐらいになっていたそうで 御座[ござ]いますよ。",
        "audio_begin": 51280,
        "audio_end": 55320
    },
    {
        "id": "f000020",
        "sentence": "もう年を老ってしまっておりましたから、",
        "sentence_with_furigana": "もう年を 老[と]ってしまっておりましたから、",
        "audio_begin": 55320,
        "audio_end": 58760
    },
    {
        "id": "f000021",
        "sentence": "まるで御隠居様のようになっていたんで御座いましょうね。",
        "sentence_with_furigana": "まるで御隠居様のようになっていたんで 御座[ござ]いましょうね。",
        "audio_begin": 58760,
        "audio_end": 64000
    },
    {
        "id": "f000022",
        "sentence": "冬、",
        "sentence_with_furigana": "冬、",
        "audio_begin": 64000,
        "audio_end": 65440
    },
    {
        "id": "f000023",
        "sentence": "炬燵の上にまあるくなって、",
        "sentence_with_furigana": "炬燵[こたつ]の上にまあるくなって、",
        "audio_begin": 65440,
        "audio_end": 67729
    },
    {
